<!DOCTYPE html>
<html lang="en">

  <head>
{{{headHtml}}}
  </head>

  <body>

    <div id="main" class="container-fluid">
      <div id="header-container">
        {{{headerHtml}}}
      </div>

      <div class="row horizontal-margin page-title">
        <div class="col-md-12">
          <h2>Collections</h2>
        </div>
      </div>


      <div id="collections" class="row">
        <div class="col-md-12">
          {{#each rows}}
          <div class="row collections-row">
            <div class="col-md-10 col-md-offset-1">
              {{#each invitations}}
              <div class="collections-item">
                <a href="/collections/{{name}}">
                  <img src="/images/collections/{{name}}_preview.png" alt="">
                  <span class="collections-caption">{{nameUpperCase}}</span>
                </a>
              </div>
              {{/each}}
            </div>
          </div>
          {{/each}}
        </div>
      </div>
    </div>

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="javascripts/bootstrap.min.js"></script>

  </body>
</html>
