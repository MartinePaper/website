<!DOCTYPE html>
<html lang="en">

  <head>
    <meta charset='utf-8' />
    <meta http-equiv="X-UA-Compatible" content="chrome=1" />
    <meta name="description" content="Website : " />


    <link rel="stylesheet" type="text/css"  href="/bootstrap/css/bootstrap.css">

    <link rel="stylesheet" href="/font/aviano-sans/stylesheet.css" type="text/css" charset="utf-8">
    <link rel="stylesheet" href="/font/sofiapro_light/stylesheet.css" type="text/css" charset="utf-8">
    <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Didact+Gothic' type='text/css'>

    <link rel="stylesheet" href="/stylesheets/font-awesome.css">

    <link rel="stylesheet" type="text/css" href="/stylesheets/stylesheet.css">

    <title>MartinePaper | Custom Designed Wedding Invitations | Chicago, IL</title>

  </head>

  <body>

    <div id="main" class="container">
      <div id="header-container">
        {{{headerHtml}}}
      </div>

      <div class="row-fluid">
        <div class="span horizontal-margin page-title">
          <h2>Testimonials</h2>
          <p></p>
        </div>
      </div>

      {{#each testimonials}}
      {{#if notFirst}}
      <div class="row-fluid row-section-spacer">
        <hr>
      </div>
      {{/if}}
      <div class="row-fluid row-faq-section">
        <div class="span5">
          <div class="row-fluid">
            <div class="span">
              <div id="invitation-detail-image-container-{{index}}" class="invitation-detail-image-container">
                <img class="invitation-detail-image invitation-detail-a7-vertical" src="{{defaultImageUrl}}" alt="">
              </div>
            </div>
          </div>
          <div class="row-fluid">
            <div class="span">
              <div class="image-thumbnails testimonial-thumbnails-container">
                <div class="testimonial-thumbnails">
                  {{#each images}}
                  <a data-detail-size="a7" data-detail-orientation="vertical" href="#">
                    <img class="image-thumbnail" src="{{thumbImageUrl}}" data-detail-image="{{largeImageUrl}}" data-detail-target-id="invitation-detail-image-container-{{../index}}" alt="">
                  </a>
                  {{/each}}
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="span7">
          <blockquote class="testimonial">
            {{{quoteHtml}}}
            <p class="pull-right cite"><cite>{{name}}</cite></p>
          </blockquote>
        </div>
      </div>
      {{/each}}

    <script src="/javascripts/jquery-1.8.1.min.js"></script>
    <script src="/bootstrap/js/bootstrap.min.js"></script>
    <script src="/javascripts/main.js"></script>

  </body>
</html>
